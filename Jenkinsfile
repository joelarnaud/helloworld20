pipeline {
    agent any
    stages {
      stage ('build') {
        steps {
          echo "hello world"
            sleep 4
        
        }
       }
    stage ('test') {
        steps {
          echo "test step"
            sleep 6
        
        }
      }
    stage ('deploy') {
        steps {
          echo "deploy step"
            sleep 10
        
        }
      }
    }
}
